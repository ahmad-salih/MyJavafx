<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" gridLinesVisible="true" hgap="100.0" prefHeight="400.0" prefWidth="400.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints maxWidth="100.0" minWidth="0.0" prefWidth="100.0" />
      <ColumnConstraints maxWidth="100.0" minWidth="35.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <children>
      <Button fx:id="getInfo" mnemonicParsing="false" onAction="#getNames" prefWidth="100.0" text="Get" GridPane.columnIndex="1" />
      <ComboBox fx:id="comboBox" prefWidth="200.0" />
      <TextField fx:id="mytext" prefWidth="200.0" GridPane.rowIndex="1" />
      <Button fx:id="InsertData" mnemonicParsing="false" onAction="#insetName" prefWidth="100.0" text="Insert" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
</GridPane>
